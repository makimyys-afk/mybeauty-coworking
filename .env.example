# Database Configuration
# Railway автоматически предоставит DATABASE_URL при подключении PostgreSQL
DATABASE_URL=postgresql://user:password@host:5432/database

# Node Environment
NODE_ENV=production

# Port (Railway автоматически установит PORT)
PORT=3000

# Session Secret (сгенерируйте случайную строку)
SESSION_SECRET=your-secret-key-here-change-this-in-production
